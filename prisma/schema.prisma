generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model username {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  password String
  username String
}

model users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  password String
  username String @unique
}
