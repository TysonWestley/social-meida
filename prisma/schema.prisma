generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model feeds {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  content  String
  username String
}

model images {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  image    String
  username String
}

model users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  password String
  username String @unique
}
